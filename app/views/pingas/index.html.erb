<div class="container">
	<div class="map">
		<%= render partial: "shared/map" %>
	</div>
	<div class="index_list">
		<h2>Pings Near Me!</h2>
		<div class="fixed_height_ping_div">
			<ul class="ping_list">
				<% Pinga.all.each do |pinga| %>
					<li class="ping">
						<h4><%= link_to pinga.title, pinga.title %></h4>
						<p><%= pinga.description %> - <%= pinga.start_time%></p>
					</li>
				<% end %>
			</ul>
		</div>
	</div>
</div>